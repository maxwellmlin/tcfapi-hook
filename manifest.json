{
  "name": "__tcfapi Hook",
  "description": "Hook the __tcfapi function to inject custom consent settings.",
  "version": "1.0",
  "manifest_version": 2,
  "content_scripts": [
    {
      "js": [
        "injector.js"
      ],
      "matches": [
        "<all_urls>"
      ],
      "run_at": "document_start"
    }
  ],
  "icons": {
    "16": "icons/icon16.png",
    "32": "icons/icon32.png",
    "192": "icons/icon192.png",
    "512": "icons/icon512.png"
  }
}